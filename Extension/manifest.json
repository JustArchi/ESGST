{
    "author": "revilheart",
    "background": {
        "scripts": ["eventPage.js"],
        "persistent": true
    },
    "browser_action": {
        "default_icon": {
            "64": "icon.png"
        }
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.steamgifts.com/*",
                "*://*.steamtrades.com/*"
            ],
            "js": [
                "jQuery.min.js",
                "jQueryUI.min.js",
                "Parsedown.js",
                "esgst.js"
            ],
            "run_at": "document_start"
        }
    ],
    "description": "Enhances the SteamGifts/SteamTrades experience.",
    "icons": {
        "64": "icon.png"
    },
    "manifest_version": 2,
    "name": "ESGST",
    "permissions": [
        "*://*.raw.githubusercontent.com/*",
        "*://*.api.steampowered.com/*",
        "*://*.store.steampowered.com/*",
        "*://*.script.google.com/*",
        "*://*.script.googleusercontent.com/*",
        "*://*.sgtools.info/*",
        "*://*.steamcommunity.com/*",
        "*://*.steamgifts.com/*",
        "*://*.steamtrades.com/*",
        "*://*.isthereanydeal.com/*",
        "*://*.api.dropboxapi.com/*",
        "*://*.content.dropboxapi.com/*",
        "*://*.api.imgur.com/*",
        "*://*.googleapis.com/*",
        "*://*.graph.microsoft.com/*",
        "*://*.userstyles.org/*",
        "storage",
        "tabs",
        "unlimitedStorage",
        "cookies"
    ],
    "version": "7.18.2"
}
