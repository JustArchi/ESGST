{
    "author": "revilheart",
    "background": {
        "scripts": [
            "js/jszip.min.js",
            "eventPage.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_icon": {
            "64": "icon.png"
        }
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.steamgifts.com/*",
                "*://*.steamtrades.com/*"
            ],
            "css": [
                "css/bootstrap.min.css",
                "css/bootstrap-select.min.css",
                "css/awesome-bootstrap-checkbox.min.css",
                "css/query-builder.min.css"
            ],
            "js": [
                "js/jquery.min.js",
                "js/jquery-ui.min.js",
                "js/bootstrap.min.js",
                "js/bootstrap-select.min.js",
                "js/interact.min.js",
                "js/jszip.min.js",
                "js/parsedown.js",
                "js/query-builder.min.js",
                "esgst.js"
            ],
            "run_at": "document_start"
        }
    ],
    "description": "Enhances the SteamGifts/SteamTrades experience.",
    "icons": {
        "64": "icon.png"
    },
    "manifest_version": 2,
    "name": "ESGST",
    "permissions": [
        "*://*.raw.githubusercontent.com/*",
        "*://*.api.steampowered.com/*",
        "*://*.store.steampowered.com/*",
        "*://*.script.google.com/*",
        "*://*.script.googleusercontent.com/*",
        "*://*.sgtools.info/*",
        "*://*.steamcommunity.com/*",
        "*://*.steamgifts.com/*",
        "*://*.steamtrades.com/*",
        "*://*.isthereanydeal.com/*",
        "*://*.api.dropboxapi.com/*",
        "*://*.content.dropboxapi.com/*",
        "*://*.api.imgur.com/*",
        "*://*.googleapis.com/*",
        "*://*.graph.microsoft.com/*",
        "*://*.userstyles.org/*",
        "storage",
        "tabs",
        "unlimitedStorage",
        "cookies"
    ],
    "version": "7.20.2"
}
